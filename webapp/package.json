{
  "name": "testable-webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@formatjs/intl-relativetimeformat": "^8.0.4",
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "css-loader": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "esprima": "^4.0.1",
    "fbemitter": "^3.0.0",
    "firebase": "^8.2.7",
    "firebaseui": "^3.6.0",
    "intro.js": "^2.9.3",
    "intro.js-react": "^0.3.0",
    "material-design-lite": "^1.2.0",
    "node-sass": "^4.0.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.1",
    "react-codemirror": "^1.0.0",
    "react-detect-offline": "^2.4.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-intl": "^5.12.3",
    "react-loading-overlay": "^1.0.1",
    "react-modal": "^3.12.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-router-transition": "^1.3.0",
    "react-scripts": "4.0.2",
    "react-share": "^4.3.1",
    "react-test-renderer": "^17.0.1",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.3",
    "serve": "^11.3.2",
    "tailwindcss": "^0.7.4",
    "typed.js": "^2.0.10",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "post:css": "postcss -c postcss.config.js src/css/tailwind.css -o src/css/index.css",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "debug": "export REACT_APP_DEBUG=true && npm run tailwind:css && react-scripts start",
    "start": "npm run tailwind:css && react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "npm run lint && npm run rebuild-sass && npm run tailwind:css && npm run docs:build && export NODE_ENV=production && react-scripts build",
    "test": "react-scripts test --env=jsdom --verbose --maxWorkers=4 --colors",
    "coverage": "react-scripts test --env=jsdom --no-cache --collectCoverage --ci --watchAll=false --colors",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "eject": "react-scripts eject",
    "tailwind:css": "tailwind build src/scss/tailwind.src.scss -c src/tailwind.js -o src/css/tailwind.css && npm run post:css",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint-report": "eslint src/**/*.js -f node_modules/eslint-html-reporter/reporter.js -o eslint/report.html",
    "coveralls": "react-scripts test --coverage --coverageReporters=text-lcov | coveralls",
    "rebuild-sass": "npm rebuild node-sass",
    "docs": "styleguidist server",
    "docs:build": "styleguidist build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/codemirror": "0.0.108",
    "@types/enzyme": "^3.10.8",
    "@types/esprima": "^4.0.2",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.30",
    "@types/react": "^17.0.2",
    "@types/react-codemirror": "^1.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^5.0.16",
    "@types/react-intl": "^3.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "coveralls": "^3.1.0",
    "cypress": "^6.5.0",
    "gh-pages": "^2.2.0",
    "react-styleguidist": "^11.1.5",
    "source-map-explorer": "^2.5.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/registerServiceWorker.js",
      "!src/__test__/**/*.{js,ts}"
    ],
    "watchPathIgnorePatterns": [
      "./(build|docs|node_modules)/"
    ]
  },
  "engines": {
    "node": "12.x"
  }
}
