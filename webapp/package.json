{
  "name": "testable",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "esprima": "^4.0.1",
    "fbemitter": "^2.1.1",
    "firebase": "^5.11.1",
    "firebaseui": "^3.6.0",
    "intro.js": "^2.9.3",
    "intro.js-react": "^0.2.0",
    "material-design-lite": "^1.2.0",
    "react": "^16.9.0",
    "react-codemirror": "^1.0.0",
    "react-detect-offline": "^2.4.0",
    "react-dom": "^16.9.0",
    "react-ga": "^2.5.7",
    "react-intl": "^3.1.13",
    "react-loading-overlay": "^1.0.1",
    "react-modal": "^3.9.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-router-transition": "^1.3.0",
    "react-scripts": "^2.1.8",
    "react-share": "^3.0.1",
    "react-test-renderer": "^16.9.0",
    "redux": "^4.0.1",
    "tailwindcss": "^0.7.4",
    "typed.js": "^2.0.10"
  },
  "scripts": {
    "post:css": "postcss -c postcss.config.js src/css/tailwind.css -o src/css/index.css",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "debug": "export REACT_APP_DEBUG=true && npm run tailwind:css && react-scripts start",
    "start": "npm run tailwind:css && react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "npm run lint && npm run tailwind:css && export NODE_ENV=production && react-scripts build && npm run docz:build",
    "test": "react-scripts test --env=jsdom --verbose --maxWorkers=4",
    "coverage": "react-scripts test --env=jsdom --no-cache --collectCoverage",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "eject": "react-scripts eject",
    "tailwind:css": "tailwind build src/scss/tailwind.src.scss -c src/tailwind.js -o src/css/tailwind.css && npm run post:css",
    "lint": "eslint src/**/*.js",
    "coveralls": "react-scripts test --coverage --coverageReporters=text-lcov | coveralls",
    "type:check": "tsc --noEmit",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "stryker:run": "stryker run"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@formatjs/intl-relativetimeformat": "^4.0.1",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@stryker-mutator/core": "^2.0.2",
    "@stryker-mutator/html-reporter": "^2.0.2",
    "@stryker-mutator/javascript-mutator": "^2.0.2",
    "@stryker-mutator/jest-runner": "^2.0.2",
    "@types/codemirror": "^0.0.81",
    "@types/enzyme": "^3.10.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/esprima": "^4.0.2",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.0",
    "@types/react-codemirror": "^1.0.2",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "babel-eslint": "^9.0.0",
    "codacy-coverage": "^3.4.0",
    "coveralls": "^3.0.4",
    "css-loader": "^3.2.0",
    "cypress": "^3.2.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.18",
    "postcss-cli": "^6.1.2",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.3.2",
    "source-map-explorer": "^2.1.1",
    "stryker-cli": "^1.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/registerServiceWorker.js",
      "!src/__test__/**/*.js"
    ]
  },
  "engines": {
    "node": "10.x"
  }
}
